function largestOfFour(arr) {
var biggestNums = [];
for (i=0;i<arr.length;i++) {
  var maxByArray = Math.max.apply(Math,arr[i]); //Math used as context and arr[i] as argument to get the max from
  bigNums.push(maxByArray);
}
      return biggestNums;
    }

    largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
